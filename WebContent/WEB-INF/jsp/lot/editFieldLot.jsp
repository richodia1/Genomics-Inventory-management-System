<%@ include file="/common/taglibs.jsp"%>
<tr>
	<td class="label"><label>Planting date:</label></td>
	<td><iita:datepicker name="lot.plantingDate" value="%{lot.plantingDate}" /></td>
</tr>
<tr>
	<td class="label"><label>Peg number:</label></td>
	<td><s:textfield name="lot.pegNumber" value="%{lot.pegNumber}" cssClass="numeric-input" /></td>
</tr>
<tr>
	<td class="label"><label>Geoposition:</label></td>
	<td><s:textfield tooltip="Latitude" name="lot.latitude" value="%{lot.latitude}" cssClass="numeric-input" />, <s:textfield name="lot.longitude" value="%{lot.longitude}" tooltip="Longitude" cssClass="numeric-input" /></td>
	<td class="label"><label>Geoposition date:</label></td>
	<td><iita:datepicker name="lot.geoposDate" value="%{lot.geoposDate}" /></td>
</tr>
